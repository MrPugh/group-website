---
layout: none
---
BEGIN:VCALENDAR
VERSION:2.0
PRODID:{{ site.url }}
NAME:Seminars
X-WR-CALNAME:Seminars
REFRESH-INTERVAL;VALUE=DURATION:PT24H
X-PUBLISHED-TTL:PT24H
X-WR-TIMEZONE:Europe/Copenhagen:
METHOD:PUBLISH
{%- for seminar in site.data.seminars %}
BEGIN:VEVENT
{%- capture this_id %}{{ seminar.date | date: "%y-%m-%d" }}-{{ seminar.title | handleize }}{% endcapture %}
DTSTAMP;TZID=Europe/Copenhagen:{{ "now" | date: "%Y%m%dT%H%M%S" }}
SEQUENCE:{{ seminar.sequence | default:0 }}
UID:{{ site.url }}seminars_and_visitors.html#{{ this_id }}
  {%- if seminar.organiser %}
ORGANIZER;CN={{ seminar.organiser.name }}:MAILTO:{{ seminar.organiser.email }}
  {%- else %}
ORGANIZER;CN=Lu√≠s Cruz-Filipe:MAILTO:lcf@imada.sdu.dk
  {%- endif %}
DTSTART;TZID=Europe/Copenhagen:{{ seminar.date | date: "%Y%m%dT%H%M%S" }}
{%- assign duration=seminar.duration | default: 3600 %}
DTEND;TZID=Europe/Copenhagen:{{ seminar.date | date:'%s' | plus: duration | date: "%Y%m%dT%H%M%S" }}
SUMMARY:{{ seminar.title }}
LOCATION:{{ seminar.place }}
DESCRIPTION:SPEAKER: {{ seminar.speaker.name }} ({{ seminar.speaker.affiliation }}) \nABSTRACT: {{ seminar.description | plainify | strip_newlines }} \nURL: {{ site.url }}/seminars_and_visitors.html#{{ this_id }}{% if seminar.place_link %} \nMAP: {{ seminar.place_link }} {% endif %}
END:VEVENT
{%- endfor %}
END:VCALENDAR