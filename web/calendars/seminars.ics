---
layout: none
---
BEGIN:VCALENDAR
VERSION:2.0
PRODID:{{ site.url }}
NAME:Seminars
X-WR-CALNAME:Seminars
REFRESH-INTERVAL;VALUE=DURATION:PT24H
X-PUBLISHED-TTL:PT24H
METHOD:PUBLISH
{%- for seminar in site.data.seminars %}
BEGIN:VEVENT
{%- capture this_id %}{{ seminar.date | date: "%y-%m-%d" }}-{{ seminar.title | handleize }}{% endcapture %}
DTSTAMP:{{ "now" | date: "%Y%m%dT%H%M%S" }}
SEQUENCE:{{ seminar.sequence | default:0 }}
UID:{{ site.url }}seminars_and_visitors.html#{{ this_id }}
  {%- if seminar.organiser %}
ORGANIZER;CN={{ seminar.organiser.name }}:MAILTO:{{ seminar.organiser.email }}
  {%- else %}
ORGANIZER;CN=Lu√≠s Cruz-Filipe:MAILTO:lcf@imada.sdu.dk
  {%- endif %}
DTSTART:{{ seminar.date | date: "%Y%m%dT%H%M%S" }}
DTEND:{{ seminar.date | date:'%s' | plus:3600 | date: "%Y%m%dT%H%M%S" }}
SUMMARY:{{ seminar.title }}
LOCATION:{{ seminar.place }}
DESCRIPTION:Speaker: {{ seminar.speaker.name }} ({{ seminar.speaker.affiliation }})\nAbstract: {{ seminar.description | plainify }}\nURL: {{ site.url }}/seminars_and_visitors.html#{{ this_id }}{% if seminar.place_link %}/nMAP: {{ seminar.place_link }} {% endif %}
END:VEVENT
{%- endfor %}
END:VCALENDAR